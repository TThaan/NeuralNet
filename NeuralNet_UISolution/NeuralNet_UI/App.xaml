<Application x:Class="NeuralNet_UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:NeuralNet_UI"
             xmlns:conv="clr-namespace:NeuralNet_UI.Converters">
    
    <Application.Resources>
        
        <conv:TestConverter x:Key="TestConverter"/>
        <conv:BoolReversingConverter x:Key="BoolReversingConverter"/>
        
        <sys:Boolean x:Key="True">True</sys:Boolean>
        <sys:Boolean x:Key="False">False</sys:Boolean>

    </Application.Resources>
    
</Application>
